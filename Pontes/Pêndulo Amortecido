PÃªndulo amortecido

function derx1= f1(omega)
derx1=omega;
endfunction

function derx2= f2(teta,omega)
derx2=-(9.81*sin(teta))-(omega);
endfunction

w1 = 0; a1 = pi/2; W = zeros(1,2); A = zeros(1,2); w2 = 0; a2 = 3*pi/2;
for i = [1:1000]
p = 0.1;
w1 = w1 + p*f2(a1,w1);
a1 = a1 + p*f1(w1);
w2 = w2 + p*f2(a2,w2);
a2 = a2 + p*f1(w2);
W(i,1) = w1; A(i,1) = a1; W(i,2) = w2; A(i,2) = a2;
end
